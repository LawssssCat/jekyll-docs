// see https://cn.eslint.org/
{
  "env": {
      "browser": true,
      "jquery": true,
      "node": true,
      "es6": true
  },
  "rules": {
      "no-console": "warn",
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "comma-dangle": ["error", "never"],
      "block-scoped-var": "error",
      "default-case": "error",
      "no-extra-bind": "error",
      "camelcase": "error",
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "eol-last": ["error", "always"]
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["eslint:recommended"]
}
